<ul class='input-list test-list'>
  <% JSON.parse(sequence_result.input_params).each do |type, value| %>
    <li class='test-list-item'>
    <span>
        <strong>
        <%= type.sub('_', ' ').titlecase %> &nbsp;
        </strong>
        <%= html_escape(value) %>
    </span>
    </li>
  <% end %>
</ul>

